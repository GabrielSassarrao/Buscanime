{
  "expo": {
    "name": "Buscanime",
    "slug": "buscanime",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1A2F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabriel.buscanime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1A2F"
      },
      "package": "com.gabriel.buscanime"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "buscanime",
    "extra": {
      "eas": {
        "projectId": "e7a7fac3-6444-4211-89b9-4dbce418f4a7"
      }
    }
  }
}